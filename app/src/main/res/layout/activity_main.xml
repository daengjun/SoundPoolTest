<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/Beep1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="실패 길게 진동"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Beep2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="성공 짧게 진동"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Beep1" />

    <Button
        android:id="@+id/Beep3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="실패 짧게 진동"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Beep2"/>

    <Button
        android:id="@+id/Beep4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="중복스캔 짧게 두번"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Beep3"/>

</androidx.constraintlayout.widget.ConstraintLayout>